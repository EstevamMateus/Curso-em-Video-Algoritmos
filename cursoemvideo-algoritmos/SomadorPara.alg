algoritmo "somador"
// Função :
// Autor :
// Data : 17/06/2021
// Seção de Declarações 
var
     N, C, S: inteiro
inicio
       S <- 0
       para C <- 1 ate 5 faca
                escreval("Digite um valor: ")
                leia(N)
                S <- S + N
       FimPara
       Escreval(" A soma dos valores é ", S)
fimalgoritmo
